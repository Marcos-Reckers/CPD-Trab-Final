{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "gpp build active file linux",
            "command": "/usr/bin/g++",
            "args": [
                "-std=c++20",
                "-Wall",
                "-fdiagnostics-color=always",
                // "-fanalyzer",
                "-Wno-analyzer-use-of-uninitialized-value",
                "-g",
                "${fileDirname}\\classes\\*.cpp",
                "${fileDirname}\\classes\\Trees\\*.cpp",
                "${fileDirname}\\*.cpp",
                "-fopenmp",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-lm",
                "-I${fileDirname}",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
